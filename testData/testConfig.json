{
    "user-namespace": "jupyter-pods",
    "sub-dir": "/lw-workspace",
    "user-volume-size": "10Gi",
    "sidecar": {
      "cpu-limit": "1.0",
      "memory-limit": "256Mi",
      "image": "quay.io/cdis/gen3fuse-sidecar:chore_sidecar",
      "env": {"NAMESPACE":"default", "HOSTNAME": "niaid.bionimbus.org"},
      "args": [],
      "command": ["/bin/bash", "/sidecarDockerrun.sh"],
      "lifecycle-pre-stop": ["su", "-c", "echo test", "-s", "/bin/sh", "root"]
    },
    "containers": [{
      "target-port": 8787,
      "cpu-limit": "1.0",
      "memory-limit": "1024Mi",
      "name": "R Studio",
      "image": "quay.io/cdis/rstudio:master",
      "env": {"DISABLE_AUTH": "true"},
      "args": [],
      "path-rewrite": "/",
      "use-tls": "false",
      "ready-probe": "/",
      "user-volume-location": "/home/rstudio/pd",
      "fs-gid": 100
    },{
      "target-port": 8888,
      "cpu-limit": "1.0",
      "memory-limit": "1024Mi",
      "name": "Jupyter - Python/R",
      "image": "quay.io/occ_data/jupyternotebook:1.7.2",
      "env": {},
      "args": ["--NotebookApp.base_url=/lw-workspace/proxy/","--NotebookApp.password=''","--NotebookApp.token=''"],
      "command": ["start-notebook.sh"],
      "path-rewrite": "/lw-workspace/proxy/",
      "use-tls": "false",
      "ready-probe": "/lw-workspace/proxy/",
      "lifecycle-post-start": ["/bin/sh","-c","export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; ln -s $(pwd) /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; ln -s /data /home/$IAM/pd/; true"],
      "user-uid": 1000,
      "fs-gid": 100,
      "user-volume-location": "/home/jovyan/pd"
    },{
      "target-port": 8888,
      "cpu-limit": "1.0",
      "memory-limit": "15512Mi",
      "name": "Jupyter - Ariba and Mykrobe",
      "image": "quay.io/cdis/niaid-jupyterhub:0.1.2",
      "env": {},
      "args": ["--NotebookApp.base_url=/lw-workspace/proxy/","--NotebookApp.password=''","--NotebookApp.token=''"],
      "command": ["start-notebook.sh"],
      "path-rewrite": "/lw-workspace/proxy/",
      "use-tls": "false",
      "ready-probe": "/lw-workspace/proxy/",
      "lifecycle-post-start": ["/bin/sh","-c","export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; ln -s $(pwd) /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; ln -s /data /home/$IAM/pd/; true"],
      "user-uid": 1000,
      "fs-gid": 100,
      "user-volume-location": "/home/jovyan/pd"
    },
    {
      "target-port": 8000,
      "cpu-limit": "0.8",
      "memory-limit": "256Mi",
      "name": "Test MultiContainer App",
      "image": "python:3.8-buster",
      "env": {},
      "args": [
        "-c",
        "cd /tmp; echo '<html><body>Hello!</body></html>' > index.html; /usr/bin/python3 -m http.server 8000"
      ],
      "command": [
        "/bin/bash"
      ],
      "path-rewrite": "/lw-workspace/proxy/",
      "use-tls": "false",
      "ready-probe": "/index.html",
      "lifecycle-post-start": [
        "/bin/sh",
        "-c",
        "echo 'hello';"
      ],
      "user-uid": 1000,
      "fs-gid": 100,
      "user-volume-location": "/tmp",
      "friends": [
        {
          "name": "friend1",
          "env": [
            {
              "name": "bla",
              "value": "friend1"
            }
          ],
          "resources": {
            "requests": {
              "cpu": 0.25
            },
            "limits": {
              "cpu": 2,
              "memory": "250Mi"
            }
          },
          "readinessProbe": {
            "exec": {
              "command": [
                "cat",
                "/tmp/healthy"
              ],
              "initialDelaySeconds": 5,
              "periodSeconds": 10
            }
          },
          "livenessProbe": {
            "exec": {
              "command": [
                "cat",
                "/tmp/healthy"
              ],
              "initialDelaySeconds": 5,
              "periodSeconds": 10
            }
          },
          "args": [
            "-c",
            "while true; do echo $bla; touch /tmp/healthy; sleep 30; done"
          ],
          "command": [
            "/bin/bash"
          ],
          "image": "ubuntu:18.04"
        },
        {
          "name": "friend2",
          "env": [
            {
              "name": "bla",
              "value": "friend2"
            }
          ],
          "resources": {
            "requests": {
              "cpu": 0.25
            },
            "limits": {
              "cpu": 2,
              "memory": "250Mi"
            }
          },
          "readinessProbe": {
            "exec": {
              "command": [
                "cat",
                "/tmp/healthy"
              ],
              "initialDelaySeconds": 5,
              "periodSeconds": 10
            }
          },
          "livenessProbe": {
            "exec": {
              "command": [
                "cat",
                "/tmp/healthy"
              ],
              "initialDelaySeconds": 5,
              "periodSeconds": 10
            }
          },
          "args": [
            "-c",
            "while true; do echo $bla; touch /tmp/healthy; sleep 30; done"
          ],
          "command": [
            "/bin/bash"
          ],
          "image": "ubuntu:18.04"
        }
      ]
    }    
  ],
  "more-configs": [
    {
      "type": "dockstore-compose:1.0.0",
      "path": "../testData/dockstore/docker-compose.yml",
      "name": "DockstoreTest"
    }
  ]
}

